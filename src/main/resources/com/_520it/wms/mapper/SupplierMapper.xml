<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com._520it.wms.mapper.SupplierMapper">
  <resultMap id="BaseResultMap" type="com._520it.wms.domain.Supplier">
    <id column="id" property="id" />
    <result column="name" property="name" />
    <result column="phone" property="phone" />
    <result column="address" property="address" />
  </resultMap>
  <delete id="deleteByPrimaryKey" parameterType="java.lang.Long">
    delete from `supplier`
    where id = #{id}
  </delete>
  <insert id="insert" keyProperty="id" parameterType="com._520it.wms.domain.Supplier" useGeneratedKeys="true">
    insert into `supplier` (name, phone, address)
    values (#{name}, #{phone}, #{address})
  </insert>
  <update id="updateByPrimaryKey" parameterType="com._520it.wms.domain.Supplier">
    update `supplier`
    set name = #{name},
      phone = #{phone},
      address = #{address}
    where id = #{id}
  </update>
  <select id="selectByPrimaryKey" parameterType="java.lang.Long" resultMap="BaseResultMap">
    select id, name, phone, address
    from `supplier`
    where id = #{id}
  </select>
  <select id="selectAll" resultMap="BaseResultMap">
    select id, name, phone, address
    from `supplier`
  </select>

  <select id="queryForCount" resultType="int">
    select count(id)
    from `supplier`
  </select>
  <select id="queryForList" resultMap="BaseResultMap">
    select id, name, phone, address
    from `supplier`
    limit #{start},#{pageSize}
  </select>
</mapper>